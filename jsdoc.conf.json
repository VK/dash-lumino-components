{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "source": {
        "include": [
            "src/lib/components/"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "systemName": "Dash Lumino Components",
        "systemSummary": "Integrate Lumino Widgets into Plotly's Dash to build interactive web applications",
        "collapseSymbols": false,
        "showTableOfContents": false
    },
    "opts": {
        "destination": "docs",
        "recurse": true,
        "readme": "README.md",
        "private": false,
        "template": "node_modules/clean-jsdoc-theme",
        "theme_opts": {
            "menu": [
                {
                    "title": "Github",
                    "link": "https://github.com/VK/dash-lumino-components",
                    "target": "_blank"
                }
            ]
        }
    }
}